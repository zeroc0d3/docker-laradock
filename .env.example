# ================================================================================================
PGADMIN_VERSION=3.0
PGADMIN_PORT=8086
PGADMIN_DEFAULT_EMAIL=laradock@laradock.io
PGADMIN_DEFAULT_PASSWORD=password

# ================================================================================================
